{
  "name": "smh-web-bot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint . --max-warnings=0",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "brand:guard": "node brand-guard.cjs",
    "brand:audit": "node scripts/brand-audit.cjs",
    "brand:report": "node scripts/brand-report.cjs",
    "verify:hue": "node scripts/verify-hue.cjs",
    "verify:brand": "pnpm exec playwright test tests/brand-lock.spec.ts",
    "verify:visual": "pnpm exec playwright test preview/brand-live.spec.ts",
    "hue:capture": "node scripts/capture-gradient.cjs",
    "hue:lock": "node scripts/lock-gradient.cjs",
    "prepush": "pnpm run brand:guard && pnpm run verify:hue && pnpm test:brand",
    "prepare": "husky",
    "test:brand": "pnpm exec playwright test tests/brand-lux.spec.ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.24",
    "@fontsource-variable/inter": "^5.2.8",
    "@fontsource-variable/playfair-display": "^5.2.8",
    "@react-three/drei": "^10.7.4",
    "@react-three/fiber": "^9.3.0",
    "@types/three": "^0.180.0",
    "ai": "^5.0.33",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "next-seo": "^6.8.0",
    "openai": "^5.19.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "stripe": "^18.5.0",
    "three": "^0.180.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.51.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "10.4.20",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "next-sitemap": "^4.2.3",
    "postcss-import": "^16.1.1",
    "postcss": "^8.4.47",
    "tailwindcss": "^4",
    "tsx": "^4.19.0",
    "typescript": "^5",
    "husky": "^9.1.7"
  },
  "engines": {
    "node": ">=20"
  }
}
